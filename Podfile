# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Image Library' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Image Library
  pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git', :tag => '4.6.0'
  pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git', :tag => '4.0.0'
  pod 'SDWebImage', :git => 'https://github.com/rs/SDWebImage.git', :tag => '4.2.2'
  pod 'Gloss', '~> 2.0.1'
  target 'Image LibraryTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Image LibraryUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'Image List Library' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Image List Library

  target 'Image List LibraryTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
